<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
										xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
										xmlns:view="clr-namespace:Dungeoneer.View"
										xmlns:model="clr-namespace:Dungeoneer.Model">
	<DataTemplate x:Key="PlayerActorDisplayTemplate" DataType="{x:Type model:PlayerActor}">
		<view:PlayerActorCard/>
	</DataTemplate>
	<DataTemplate x:Key="NonPlayerActorDisplayTemplate" DataType="{x:Type model:NonPlayerActor}">
		<view:NonPlayerActorCard/>
	</DataTemplate>
	<DataTemplate x:Key="CreatureInitiativeTemplate" DataType="{x:Type model:Creature}">
		<view:CreatureInitiativeCard/>
	</DataTemplate>
</ResourceDictionary>
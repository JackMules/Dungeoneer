<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
										xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
										xmlns:local="clr-namespace:Dungeoneer">
	<DataTemplate x:Key="ActorDisplayTemplate" DataType="{x:Type local:Actor}">
		<local:CharacterCard/>
	</DataTemplate>
	<DataTemplate x:Key="CreatureInitiativeTemplate" DataType="{x:Type local:Creature}">
		<local:CreatureCard/>
	</DataTemplate>
</ResourceDictionary>